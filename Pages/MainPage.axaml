<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="BankAccount.MainPage">
	<ContentControl x:Name="MainContent">
		<TabControl Grid.Row="0" HorizontalAlignment="Left" VerticalAlignment="Top" Width="1276" MinHeight="1020" Background="">
			<TabItem Header="BankAccounts">
				<StackPanel Orientation="Horizontal">
					<Grid x:Name="gridBankAccount" Loaded="BankAccountLoaded" ColumnDefinitions="*,*" RowDefinitions="*,*,*,*,*,*,*,*">
						
						<Button Click="CreateNewBankAccount" Margin="40,0,0,0" CornerRadius="20" Grid.Column="1" Grid.Row="0" FontSize="32" Width="500" Height="70">
							<Label HorizontalAlignment="Center">Create New Bank Account</Label>
						</Button>
					</Grid>
					
				</StackPanel>
			</TabItem>
			<TabItem Header="Loans">
				
			</TabItem>
			<TabItem Header="Settings">

			</TabItem>
		</TabControl>
	</ContentControl>
</UserControl>
